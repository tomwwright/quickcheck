{
  "compilerOptions": {
    "lib": ["es6", "dom", "esnext"],
    "target": "es2016",
    "module": "commonjs",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "outDir": ".build",
    "baseUrl": ".",
    "paths": {
      "quickcheck": ["../quickcheck-types/build/"]
    }
  },
  "include": ["*.ts", "**/*.ts"],
  "exclude": ["node_modules"],
  "references": [{ "path": "../quickcheck-types" }]
}
